{
  "name": "k8sight",
  "private": true,
  "productName": "K8Sight",
  "description": "K8Sight - Free IDE for Kubernetes",
  "homepage": "https://k8sight.app",
  "version": "1.7.0-0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GlobalArtInc/k8sight.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/GlobalArtInc/k8sight/issues"
  },
  "main": "static/build/main.js",
  "copyright": "Â© 2024-2025 GlobalArt Inc.",
  "license": "MIT",
  "author": {
    "name": "GlobalArt Inc.",
    "email": "tech@globalart.dev"
  },
  "scripts": {
    "clean": "corepack pnpm dlx rimraf@6.1.2 binaries dist static/build",
    "clean:node_modules": "corepack pnpm dlx rimraf@6.1.2 node_modules",
    "build": "webpack --config webpack/webpack.ts --progress",
    "postbuild": "corepack pnpm build:resources",
    "build:dev": "NODE_ENV=development webpack --config webpack/webpack.ts --progress",
    "postbuild:dev": "corepack pnpm build:resources",
    "build:resources": "corepack pnpm \"/^build:resources:/\"",
    "build:resources:client": "ensure-binaries --package package.json --base-dir binaries/client",
    "build:resources:license": "generate-license --prod --header license-header.txt --output static/build/license.txt",
    "build:resources:tray": "generate-tray-icons --output static/build/tray --input @kubesightapp/icon/icons/logo-k8sight.svg --notice-icon @kubesightapp/icon/icons/notice.svg --spinner-icon @kubesightapp/icon/icons/arrow-spinner.svg --none-color fbfbfb",
    "prebuild:app": "corepack pnpm build && corepack pnpm electron-rebuild",
    "build:app": "corepack pnpm dlx run-script-os@1.1.6 --",
    "build:app:darwin": "electron-builder --publish never --macos",
    "build:app:default": "electron-builder --publish never",
    "build:app:linux": "electron-builder --publish never --linux",
    "build:app:win32": "electron-builder --publish never --win",
    "electron-rebuild": "electron-rebuild -f",
    "jest": "jest",
    "start": "corepack pnpm dlx run-script-os@1.1.6 --",
    "start:darwin": "corepack pnpm start:darwin:${npm_config_arch:-$(uname -m)}",
    "start:darwin:arm64": "./dist/mac-arm64/K8Sight.app/Contents/MacOS/K8Sight",
    "start:darwin:x64": "./dist/mac/K8Sight.app/Contents/MacOS/K8Sight",
    "start:darwin:x86_64": "./dist/mac/K8Sight.app/Contents/MacOS/K8Sight",
    "start:default": "exit 1",
    "dev": "NODE_ENV=development corepack pnpm \"/^dev:/\"",
    "dev:electron": "corepack pnpm dlx rimraf@6.1.2 ./static/build/k8sight.js ./static/build/index.html && corepack pnpm dlx chokidar-cli@3.0.0 ./static/build/k8sight.js ./static/build/index.html --throttle 5000 --command \"corepack pnpm start:electron\"",
    "dev:webpack": "webpack --config webpack/main.ts --progress --watch",
    "dev:server": "ts-node ./webpack/dev-server.ts",
    "start:electron": "electron --remote-debugging-port=9223 --inspect .",
    "start:linux": "corepack pnpm start:linux:${npm_config_arch:-$(uname -m)}",
    "start:linux:arm64": "./dist/linux-arm64-unpacked/k8sight",
    "start:linux:x64": "./dist/linux-unpacked/k8sight",
    "start:linux:x86_64": "./dist/linux-unpacked/k8sight",
    "start:win32": "dist\\win-unpacked\\K8Sight.exe",
    "test:integration": "jest -xyz --runInBand --detectOpenHandles --forceExit"
  },
  "config": {
    "k8sProxyVersion": "1.0.1",
    "bundledKubectlVersion": "1.34.2",
    "bundledHelmVersion": "4.0.1",
    "contentSecurityPolicy": "script-src 'unsafe-eval' 'self'; frame-src https://*.renderer.k8sight.app:*/; img-src * data:",
    "welcomeRoute": "/welcome"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "dependencies": {
    "@astronautlabs/jsonpath": "^1.1.2",
